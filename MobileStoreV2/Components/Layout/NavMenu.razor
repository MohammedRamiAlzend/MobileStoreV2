@using MobileStoreV2.Components.Pages
@using MobileStoreV2.Components.Pages.AllDashboard
@using MobileStoreV2.Components.Pages.ProductsCmpnt



@*     <MudRTLProvider RightToLeft="@_rightToLeft"> *@

@if (_rightToLeft == false)
{
    <MudNavMenu>
        <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.Store" Href="ProductsList"> Products</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.LibraryBooks" Href="Dashboard"> Dashboard</MudNavLink>
        <MudNavLink Match="NavLinkMatch.All" Icon="@Icons.Material.Filled.AccountBox" Href="Accounting">Accounting</MudNavLink>
